{
  "name": "angular-stencil-starter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "pnpm build:web-components && pnpm build:angular-components && pnpm build:react-components && pnpm build:angular-app && pnpm build:react-app",
    "build:web-components": "pnpm --filter web-components build",
    "build:angular-components": "pnpm --filter angular-components build",
    "build:react-components": "pnpm --filter react-components build",
    "build:angular-app": "pnpm --filter angular-app build",
    "build:react-app": "pnpm --filter react-app build",
    "start": "pnpm --filter angular-app start",
    "start:angular": "pnpm --filter angular-app start",
    "start:react": "pnpm --filter react-app start",
    "start:web-components": "pnpm --filter web-components start",
    "dev": "pnpm build:web-components && pnpm build:angular-components && pnpm start",
    "dev:react": "pnpm build:web-components && pnpm build:react-components && pnpm start:react",
    "test": "pnpm -r test",
    "clean": "rm -rf node_modules packages/*/node_modules packages/angular-components/dist packages/react-components/dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0"
}
