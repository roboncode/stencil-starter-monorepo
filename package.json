{
  "name": "angular-stencil-starter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "pnpm build:stencil-library && pnpm build:angular-library && pnpm build:app",
    "build:stencil-library": "pnpm --filter stencil-library build",
    "build:angular-library": "pnpm --filter angular-library build",
    "build:app": "pnpm --filter app build",
    "start": "pnpm --filter app start",
    "start:stencil-library": "pnpm --filter stencil-library start",
    "dev": "pnpm build:stencil-library && pnpm build:angular-library && pnpm start",
    "test": "pnpm -r test",
    "clean": "rm -rf node_modules packages/*/node_modules packages/angular-library/dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0"
}
