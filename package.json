{
  "name": "angular-stencil-starter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "pnpm build:web-components && pnpm build:angular-components && pnpm build:angular-app",
    "build:web-components": "pnpm --filter web-components build",
    "build:angular-components": "pnpm --filter angular-components build",
    "build:angular-app": "pnpm --filter angular-app build",
    "start": "pnpm --filter angular-app start",
    "start:web-components": "pnpm --filter web-components start",
    "dev": "pnpm build:web-components && pnpm build:angular-components && pnpm start",
    "test": "pnpm -r test",
    "clean": "rm -rf node_modules packages/*/node_modules packages/angular-components/dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0"
}
