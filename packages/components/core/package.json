{
  "name": "@components/core",
  "version": "0.0.1",
  "description": "Core Web Components Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core/core.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/core/core.esm.js",
      "require": "./dist/cjs/core.cjs.js"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*",
      "types": "./dist/components/*"
    },
    "./dist/components/*.js": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./ui-tooltip": {
      "import": "./dist/components/ui-tooltip.js",
      "types": "./dist/components/ui-tooltip.d.ts"
    },
    "./ui-input-group": {
      "import": "./dist/components/ui-input-group.js",
      "types": "./dist/components/ui-input-group.d.ts"
    },
    "./ui-input-group-input": {
      "import": "./dist/components/ui-input-group-input.js",
      "types": "./dist/components/ui-input-group-input.d.ts"
    },
    "./ui-input-group-addon": {
      "import": "./dist/components/ui-input-group-addon.js",
      "types": "./dist/components/ui-input-group-addon.d.ts"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./styles/themes.css": "./dist/core/styles/themes.css",
    "./styles/tokens.css": "./dist/core/styles/tokens.css",
    "./styles/*": "./dist/core/styles/*"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.40.1",
    "@stencil/react-output-target": "^1.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "puppeteer": "^24.34.0"
  },
  "license": "MIT",
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@fortawesome/fontawesome-free": "^7.1.0"
  }
}
