{
  "name": "web-components",
  "version": "0.0.1",
  "description": "Web Components Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web-components/web-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/web-components/web-components.esm.js",
      "require": "./dist/cjs/web-components.cjs.js"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*",
      "types": "./dist/components/*"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./ui-tooltip": {
      "import": "./dist/components/ui-tooltip.js",
      "types": "./dist/components/ui-tooltip.d.ts"
    },
    "./ui-input-group": {
      "import": "./dist/components/ui-input-group.js",
      "types": "./dist/components/ui-input-group.d.ts"
    },
    "./ui-input-group-input": {
      "import": "./dist/components/ui-input-group-input.js",
      "types": "./dist/components/ui-input-group-input.d.ts"
    },
    "./ui-input-group-addon": {
      "import": "./dist/components/ui-input-group-addon.js",
      "types": "./dist/components/ui-input-group-addon.d.ts"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./styles/themes.css": "./dist/web-components/styles/themes.css",
    "./styles/tokens.css": "./dist/web-components/styles/tokens.css",
    "./styles/*": "./dist/web-components/styles/*"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^1.0.0",
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@fortawesome/fontawesome-free": "^7.1.0"
  }
}
